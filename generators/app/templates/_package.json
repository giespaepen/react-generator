{
  "name": "<%= title %>",
  "version": "1.0.0",
  "description": "Simple Configurable ContentFul React CMS",
  "main": "server/app.js",
  "author": "Gie Spaepen",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p src/server && (cd src/client && webpack)",
    "build-acc": "NODE_ENV=ACC tsc -p src/server && (cd src/client && webpack --config webpack.config.prd.js)",
    "build-prd": "NODE_ENV=PRD tsc -p src/server && (cd src/client && webpack --config webpack.config.prd.js)",
    "build-ci": "tsc -p src/server && (cd src/client && webpack --bail)",
    "start": "node src/server/app.js --no-lazy",
    "start-acc": "NODE_ENV=ACC node src/server/app.js --no-lazy",
    "start-prd": "NODE_ENV=PRD node src/server/app.js --no-lazy",
    "clean": "rm ./src/client/assets/scripts/*.*",
    "test": "jest"
  },
  "jest": {
    "globals": {
      "__TS_CONFIG__": "./src/client/tsconfig.json"
    },
    "collectCoverage": true,
    "transform": {
      ".(scss)": "./src/client/__tests__/__processors__/SassImportProcessor.js",
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "testResultsProcessor": "./node_modules/ts-jest/coverageprocessor.js",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
